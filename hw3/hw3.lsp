;
; Helper function of getBoxPositions
; to get list of box coordinates in single row
(defun getBoxesInRow (row c r)
  (cond ((null row) nil)
        ((isBox (car row)) (append (list (list c r)) (getBoxesInRow (cdr row) (+ c 1) r)))
        (T (getBoxesInRow (cdr row) (+ c 1) r))))

; Helper function to get coordinates of all free boxes in game
; takes state s and a row number r showing current index of top row
; then recurses through each row and appends boxes found to coordinate list
(defun findboxes (s r)
  (cond ((null s) nil)
        (T (append (getBoxesInRow (car s) 0 r) (findboxes (cdr s) (+ r 1))))))

;
; Helper function of findgoals
; similar logic to getBoxesInRow
(defun getGoalsInRow (row c r)
  (cond ((null row) nil)
        ((isStar (car row)) (append (list (list c r)) (getGoalsInRow (cdr row) (+ c 1) r)))
        (T (getGoalsInRow (cdr row) (+ c 1) r))))

; Helper function to get coordinates of all unoccupied goals in game
; takes state s and a row number r showing current index of top row
; similar logic to findboxes
(defun findgoals (s r)
  (cond ((null s) nil)
        (T (append (getGoalsInRow (car s) 0 r) (findgoals (cdr s) (+ r 1))))))

; Helper function to check if a coordinate is invalid
; invalid means off grid or wall
(defun invalid (s coord)
  (let* ((c (car coord)) (r (cadr coord)))
    (cond ((null coord) T)
          ((or (< c 0) (< r 0)) T)
          (T (let ((spot (car (nthcdr c (car (nthcdr r s))))))
            (cond ((null spot) T)
                  ((isWall spot) T)
                  (T Nil)))))))

; Helper function to check if a box is stuck
; stuck means it is in some sort of corner, ie UP and LEFT are invalid (can't move there)
; or UP and RIGHT, DOWN and LEFT, DOWN and RIGHT
(defun isStuck (s coord)
  (let* ((c (car coord)) (r (cadr coord))
        (UP (list c (- r 1)))
        (DOWN (list c (+ r 1)))
        (LEFT (list (- c 1) r))
        (RIGHT (list (+ c 1) r)))
        (cond ((and (invalid s UP) (invalid s LEFT)) T)
              ((and (invalid s UP) (invalid s RIGHT)) T)
              ((and (invalid s DOWN) (invalid s LEFT)) T)
              ((and (invalid s DOWN) (invalid s RIGHT)) T)
              (T Nil))))
    
; Helper function to check for deadends
; adds a high cost if any box in coords list is stuck
(defun deadend (s coords)
    (cond ((null coords) 0)
          ((isStuck s (car coords)) 1000) ; high cost for a stuck box, game over
          (T (deadend s (cdr coords)))))

; Helper function to find absolute value
(defun absval (val)
  (cond ((< val 0) (- 0 val))
        (T val)))

; Helper function to calculate the min distance between a single box and all goals
; recursively finds distance between box and each goal in list, updating currmin as necessary
(defun calcdist (box goals currmin)
  (cond ((null goals) currmin)
        (T (let* ((g (car goals)) (steps (+ (absval (- (car box) (car g))) (absval (- (cadr box) (cadr g))))))
          (cond ((< steps currmin) (calcdist box (cdr goals) steps))
                (T (calcdist box (cdr goals) currmin)))))))

; Helper function to find sum of min distances between boxes and closest goals
; recusively calculates min dist to any goal using calcdist for each box in boxes list
(defun dist (boxes goals)
  (cond ((null boxes) 0)
        (T (+ (calcdist (car boxes) goals 1000) (dist (cdr boxes) goals)))))

; EXERCISE: Change the name of this function to h<UID> where
; <UID> is your actual student ID number. Then, modify this 
; function to compute an admissible heuristic value of s. 
; 
; This function will be entered in the competition.
; Objective: make A* solve problems as fast as possible.
; The Lisp 'time' function can be used to measure the 
; running time of a function call.
;
; This function measures the sums of distances between each box and its closest goal.
; However, if state results in any dead ends, discontinue search by adding large cost.
; This may be expensive, but saves node expansion time overall to eliminate dead ends.
; The heuristic is admissible because each box must be moved at least to closest goal,
; And if any box gets stuck in a corner, solution cannot be reached even in infinite steps.
(defun h804179311 (s)
  (let* ((boxes (findboxes s 0))
        (goals (findgoals s 0))
        (kpos (getKeeperPosition s 0))
        (dead (deadend s boxes)))
    (cond ((= 0 dead) (dist boxes goals))
          (T dead)))
  )

