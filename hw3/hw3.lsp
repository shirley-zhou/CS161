; Define some global variables
(setq blank 0)
(setq wall 1)
(setq box 2)
(setq keeper 3)
(setq star 4)
(setq boxstar 5)
(setq keeperstar 6)

; Some helper functions for checking the content of a square
(defun isBlank (v)
  (= v blank)
  )

(defun isWall (v)
  (= v wall)
  )

(defun isBox (v)
  (= v box)
  )

(defun isKeeper (v)
  (= v keeper)
  )

(defun isStar (v)
  (= v star)
  )

(defun isBoxStar (v)
  (= v boxstar)
  )

(defun isKeeperStar (v)
  (= v keeperstar)
  )

;
; Helper function of getKeeperPosition
;
(defun getKeeperColumn (r col)
  (cond ((null r) nil)
	(t (if (or (isKeeper (car r)) (isKeeperStar (car r)))
	       col
	     (getKeeperColumn (cdr r) (+ col 1))
	     );end if
	   );end t
	);end cond
  )

;
; getKeeperPosition (s firstRow)
; Returns a list indicating the position of the keeper (c r).
; 
; Assumes that the keeper is in row >= firstRow.
; The top row is the zeroth row.
; The first (right) column is the zeroth column.
;
(defun getKeeperPosition (s row)
  (cond ((null s) nil)
	(t (let ((x (getKeeperColumn (car s) 0)))
	     (if x
		 ;keeper is in this row
		 (list x row)
		 ;otherwise move on
		 (getKeeperPosition (cdr s) (+ row 1))
		 );end if
	       );end let
	 );end t
	);end cond
  );end defun

;
; cleanUpList (l)
; returns l with any NIL element removed.
; For example, if l is '(1 2 NIL 3 NIL), returns '(1 2 3).
;
(defun cleanUpList (L)
  (cond ((null L) nil)
	(t (let ((cur (car L))
		 (res (cleanUpList (cdr L)))
		 )
	     (if cur 
		 (cons cur res)
		  res
		 )
	     );end let
	   );end t
	);end cond
  );end 


; Helper function to count the number of boxes not on goals in state s
; works by recursively counting boxes in each row and adding them
 (defun numboxes (s)
   (cond ((null s) 0)
         (T (+ (count box (car s)) (numboxes (cdr s))))))

; EXERCISE: Modify this function to return true (t)
; if and only if s is a goal state of a Sokoban game.
; (no box is on a non-goal square)
;
; Currently, it always returns NIL. If A* is called with
; this function as the goal testing function, A* will never
; terminate until the whole search space is exhausted.
;
; This function checks whether number of free boxes is 0
(defun goal-test (s)
  (cond ((null s) nil)
        (T (= 0 (numboxes s)))) ;
);end defun

; Helper function to replace value in a single row
; appends the beginning of the row with the new object with the rest of row
(defun replace (row c object)
	(cond ((null row) nil))
	(let* ((len (length row)) (part1 (butlast row (- len c))) (part2 (nthcdr (+ 1 c) row)))
	  (append part1 (list object) part2)))

; Helper function to move things to coordinate
; finds the correct row and replaces item in it, appending to rest of state
(defun move (s c r object)
  (cond ((null s) nil)
        ((= r 0) (cons (replace (car s) c object) (cdr s))) ; base case, coord in first row
        (T (cons (car s) (move (cdr s) c (- r 1) object)))))

; Helper function to push box, similar logic to try-move function which moves keeper
(defun push (s x y dir)
  (let* ((c (+ x (car dir)))
	 (r (+ y (cadr dir))))
	 ; c r are the attempted new coordinates of box
	 ; spot shows what is currently occupying that coordinate
	 (cond ((null s) nil)
           ((or (< c 0) (< r 0)) nil) ; off the board
           (T (let ((spot (car (nthcdr c (car (nthcdr r s))))))
              (cond ((null spot) nil) ; off the board
                    ((isBlank spot) (move s c r box)) ; blank spot, move box there
                    ((isStar spot) (move s c r boxstar)) ; star spot, move box on star
                    (T nil))))))) ; wall, another box, or other invalid spot

; Helper function to figure out replacement value for keeper
(defun replacement (s x y)
 (cond ((isKeeperStar (car (nthcdr x (car (nthcdr y s))))) star)
       (T blank)))

; Helper function to try moving keeper in a direction
; dir represents the direction to move
; UP: (0 -1), DOWN: (0 1), LEFT: (-1 0), RIGHT: (1 0)
; checks if it's okay to move keeper:
; if so, either push or just move then replace keeper's position with new value
(defun try-move (s dir)
  (let* ((pos (getKeeperPosition s 0))
     (x (car pos))
     (y (cadr pos))
	 (c (+ x (car dir)))
	 (r (+ y (cadr dir)))
	 (new (replacement s x y))) ; 
	 ; set replacement value for keeper's spot, either blank or star
     (cond ((null s) nil)
           ((or (< c 0) (< r 0)) nil) ; off the board
           (T (let ((spot (car (nthcdr c (car (nthcdr r s))))))
           ; spot shows what is currently occupying that coordinate
             (cond ((null spot) nil) ; off the board
                   ((isBlank spot) (move (move s c r keeper) x y new)) ; blank spot, move keeper there then reset keeper's spot
                   ((isStar spot) (move (move s c r keeperstar) x y new)) ; star spot, move keeper on star then reset keeper's spot
                   ((isBox spot) (move (move (push s c r dir) c r keeper) x y new)) ; push box, move keeper, reset keeper's spot
                   ((isBoxStar spot) (move (move (push s c r dir) c r keeperstar) x y new)) ; push box, move keeper onto star, reset keeper's spot
                   (T nil))))))) ; wall or other invalid spot

; EXERCISE: Modify this function to return the list of 
; sucessor states of s.
;
; This is the top-level next-states (successor) function.
; Some skeleton code is provided below.
; You may delete them totally, depending on your approach.
; 
; If you want to use it, you will need to set 'result' to be 
; the set of states after moving the keeper in each of the 4 directions.
; A pseudo-code for this is:
; 
; ...
; (result (list (try-move s UP) (try-move s DOWN) (try-move s LEFT) (try-move s RIGHT)))
; ...
; 
; You will need to define the function try-move and decide how to represent UP,DOWN,LEFT,RIGHT.
; Any NIL result returned from try-move can be removed by cleanUpList.
; 
;
(defun next-states (s)
  (cleanUpList (list (try-move s '(0 -1)) (try-move s '(0 1)) (try-move s '(-1 0)) (try-move s '(1 0))))
  );

